{
  "name": "Miss No Fav Youtube Subscribed Channel Videos",
  "version": "2.0.1",
  "description": "Can watch Subscriptions between the dates you missed",
  "browser_action": {
    "default_title": "Open the popup",
    "default_icon": "ys2.png",
    "default_popup" : "popup.html"
  },
  "permissions": [
          "tabs",
          "history",
          "identity",
         "*://*.google.com/*"
        ],
  "manifest_version": 2,
  "content_security_policy":"script-src 'self' https://apis.google.com https://ajax.googleapis.com; object-src 'self'"
}
